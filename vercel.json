{
    "rewrites": [
        {
            "source": "/api/apollo/:path*",
            "destination": "/api/proxy"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/hunt",
            "schedule": "0 6 * * *"
        },
        {
            "path": "/api/cron/digest",
            "schedule": "0 7 * * *"
        }
    ]
}